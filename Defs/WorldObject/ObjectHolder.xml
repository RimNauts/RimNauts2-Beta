<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<WorldObjectDef>
		<defName>RimNauts2_ObjectHolder</defName>
		<worldObjectClass>RimNauts2.World.ObjectHolder</worldObjectClass>
		<canBePlayerHome>true</canBePlayerHome>
		<comps>
			<li Class="WorldObjectCompProperties_Abandon"/>
			<li Class="RimNauts2.World.Comps.DestroyObjectHolder_Properties">
				<label>Destroy</label>
				<desc>Blast the object out of orbit, never to be seen again.</desc>
			</li>
			<li Class="RimNauts2.World.Comps.GenerateObjectMap_Properties">
				<label>Generate map</label>
				<desc>Generate map for the object instantly.</desc>
			</li>
			<li Class="RimNauts2.World.Comps.RandomizeObjectHolder_Properties">
				<label>Randomize</label>
				<desc>Randomize properties.</desc>
			</li>
			<li Class="RimNauts2.World.Comps.ShipTargeter_Properties"/>
		</comps>
		<useDynamicDrawer>false</useDynamicDrawer>
		<expandingIcon>true</expandingIcon>
		<expandingIconTexture>RimNauts2_Transparent</expandingIconTexture>
		<expandingIconPriority>1003</expandingIconPriority>
		<texture>RimNauts2_Transparent</texture>
  	</WorldObjectDef>
</Defs>
